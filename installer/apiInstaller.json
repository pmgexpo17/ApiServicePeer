{
  "ApiCore": {
    "RepoMap": {
      "apibase": [
        "apibase", 
        "apiPeer.py", 
        "appDirector.py", 
        "appProvider.py", 
        "__init__.py"
      ], 
      "apitools": [
        "apitools", 
        "__init__.py"
      ], 
      "jobstore": [
        "jobstore", 
        "leveldbJobStore.py", 
        "__init__.py"
      ], 
      "root": [
        ".", 
        "apiAgent.py", 
        "apiservices.json", 
        "requirements.txt"
      ]
    }
  }, 
  "ApiPlan": [
    "root|%s", 
    "apibase|%s/apibase", 
    "apiservice|%s/apiservice", 
    "apitools|%s/apitools", 
    "jobstore|%s/jobstore", 
    "leveldb|%s/leveldb", 
    "log|%s/log"
  ], 
  "ApiRoot": "/apps/dev1/webapi", 
  "Domain": {
    "csvChecker": "localhost:5000", 
    "wcEmulation": "localhost:5000"
  }, 
  "Installed": [
    "ApiCore", 
    "wcEmulation", 
    "csvChecker"
  ], 
  "PipEnv": [
    "PIP_INDEX_URL|<pip_index_url>", 
    "PIP_TRUSTED_HOST|<pip_host>"
  ], 
  "RepoName": {
    "ApiCore": "ApiCore", 
    "csvChecker": "CsvCheckerApi", 
    "wcEmulation": "WcEmltnApi"
  }, 
  "Services": [
    "csvChecker", 
    "wcEmulation"
  ], 
  "csvChecker": {
    "ApiPlan": [
      "cctools|%s/apitools/cc"
    ], 
    "RepoMap": {
      "apiservice": [
        "apiservice", 
        "*.*"
      ], 
      "assets": [
        "assets", 
        "*.*"
      ], 
      "cctools": [
        "apitools/cc", 
        "*.*"
      ], 
      "sasautos": [
        "sasautos/csvcheck", 
        "*.*"
      ]
    }, 
    "SysPlan": [
      "assets|<assets_lib>", 
      "sasautos|<sasautos_lib>"
    ]
  }, 
  "wcEmulation": {
    "ApiPlan": [
      "wctools|%s/apitools/wcemltn"
    ], 
    "RepoMap": {
      "apiservice": [
        "apiservice", 
        "*.*"
      ], 
      "assets": [
        "assets", 
        "*.*"
      ], 
      "calcorder": [
        "calcorder", 
        "*.*"
      ], 
      "dataprep": [
        "sasautos/dataprep", 
        "*.*"
      ], 
      "simbatch": [
        "sasautos/simbatch", 
        "*.*"
      ], 
      "wctools": [
        "apitools/wcemltn", 
        "*.*"
      ], 
      "wrkcmp": [
        "sasautos/wrkcmp", 
        "*.*"
      ]
    }, 
    "SysPlan": [
      "assets|<assets_lib>", 
      "calcorder|<calcorder_lib>", 
      "dataprep|<dataprep_lib>", 
      "simbatch|<simbatch_lib>", 
      "wrkcmp|<wrkcmp_lib>"
    ]
  }
}
